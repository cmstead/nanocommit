{
  "name": "nanocommit",
  "version": "1.0.0",
  "description": "Commit every time your tests pass, no fuss, no muss",
  "main": "index.js",
  "bin": {
    "nanocommit": "index.js"
  },
  "nanocommit": {
    "testCommand": "node ./fake-test.js",
    "blindCommit": true
  },
  "scripts": {
    "test": "node ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmstead/nanocommit.git"
  },
  "keywords": [
    "testing",
    "tdd",
    "git",
    "commit",
    "agile-tools",
    "productivity"
  ],
